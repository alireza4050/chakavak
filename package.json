{
  "name": "chakavak",
  "version": "1.0.0",
  "description": "The Place To Share Ideas",
  "main": "app/app.js",
  "scripts": {
    "start:api": "nodemon --inspect ./api/bin/www",
    "start:app": "NODE_ENV=development webpack-dev-server",
    "start": "run-p start:*",
    "build": "NODE_ENV=production webpack -p",
    "deploy": "./deploy.sh",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "dgo:~/src/chakavak"
  },
  "keywords": [
    "Social",
    "Communications",
    "Friends"
  ],
  "author": "Alireza Asgharizadeh",
  "license": "UNLICENSED",
  "private": true,
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 9"
  ],
  "eslintIgnore": [
    "app/assets",
    "build",
    "dist"
  ],
  "nodemonConfig": {
    "execMap": {
      "js": "node --harmony"
    },
    "watch": [
      "api/"
    ],
    "env": {
      "NODE_ENV": "development",
      "DEBUG": "chakavak:*,express:application",
      "PORT": "3030",
      "SESSION_SECRET": "secret"
    },
    "ext": "js json"
  },
  "dependencies": {
    "@uirouter/angularjs": "^1.0.11",
    "ajv": "^5.5.1",
    "angular": "^1.6.7",
    "angular-file-upload": "^2.5.0",
    "body-parser": "^1.18.2",
    "bootstrap": "^3.3.7",
    "bootstrap4-tagsinput": "git+https://github.com/Nodws/bootstrap4-tagsinput.git",
    "bootstrap4rtl": "^0.1.6",
    "connect-history-api-fallback": "^1.5.0",
    "connect-mongo": "^2.0.0",
    "debug": "^3.1.0",
    "dotenv": "^4.0.0",
    "envvar": "^2.0.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "jquery": "^3.2.1",
    "load-google-maps-api-2": "^1.0.2",
    "mime": "^2.0.3",
    "mongoose": "^4.13.6",
    "mongoose-gridfs": "^0.3.2",
    "morgan": "^1.9.0",
    "multer": "^1.3.0",
    "multer-gridfs-storage": "^2.1.0",
    "mz": "^2.7.0",
    "ng-file-upload": "^12.2.13",
    "ng-infinite-scroll": "^1.3.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^4.4.0",
    "popper.js": "^1.12.9",
    "validator": "^9.1.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.16.8",
    "@types/connect-mongo": "0.0.34",
    "@types/cookie-parser": "^1.4.1",
    "@types/express": "^4.0.39",
    "@types/express-session": "^1.15.6",
    "@types/mongoose": "^4.7.28",
    "@types/node": "^8.0.57",
    "@types/passport": "^0.4.1",
    "@types/passport-local": "^1.0.32",
    "@types/passport-local-mongoose": "^4.0.11",
    "@types/validator": "^6.3.0",
    "autoprefixer": "^7.2.2",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-angularjs-annotate": "^0.8.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.2.0",
    "chalk": "^2.3.0",
    "clean-webpack-plugin": "^0.1.17",
    "compression-webpack-plugin": "^1.0.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.13.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-nodejs": "^2.0.0",
